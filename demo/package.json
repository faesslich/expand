{
  "name": "expand_demo",
  "version": "1.0.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild:css": "node-sass --source-map true --precision 6 assets -o dist",
    "build:css": "postcss dist/*.css --use autoprefixer -d dist",
    "watch:scss": "onchange \"assets/**/*.scss\" -- npm run build:css",
    "lint:js": "eslint assets/**/*.js || true",
    "prebuild:js": "npm run lint:js",
    "build:js": "uglifyjs assets/demo.js -c -m -o dist/demo.min.js",
    "watch:js": "onchange \"assets/**/*.js\" -- npm run build:js",
    "watch:all": "npm-run-all --parallel watch:scss watch:js"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.2",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.0.2",
    "postcss-cli": "^7.1.1",
    "uglify-js": "^3.3.23"
  },
  "repository": "faesslich/expand",
  "author": "Fabian Esser <post@fe-dev.com> (https://fe-dev.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/faesslich/expand/issues"
  },
  "homepage": "https://fe-dev.com/expand"
}
